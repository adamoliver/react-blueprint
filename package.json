{
  "name": "react-blueprint",
  "version": "1.0.0",
  "description": "React component blueprint",
  "license": "MIT",
  "main": "dist/index",
  "scripts": {
    "start": "webpack-dev-server --open -d --env.platform=default --progress",
    "build": "NODE_ENV=production BABEL_ENV=es ./node_modules/.bin/babel src/ --out-dir dist/ --ignore *.test.jsx",
    "test": "jest --config jest.json",
    "lint": "./node_modules/.bin/eslint --ext .jsx --ext .js ./src/"
  },
  "keywords": [
    "reactjs"
  ],
  "files": [
    "*.md",
    "src",
    "dist"
  ],
  "author": "mail@adamoliver.net",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "^4.1.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "jest": "22.1.4",
    "prop-types": "^15.5.10",
    "react-addons-test-utils": "15.6.2",
    "react-hot-loader": "^1.3.1",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "browserslist": [
    "last 2 versions",
    "not ie <= 9"
  ]
}
